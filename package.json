{
  "name": "syntrix",
  "displayName": "syntrix",
  "description": "Syntax highlighting and language support for Syntrix",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dokstack/syntrix.git"
  },
  "contributes": {
    "languages": [{
      "id": "syntrix",
      "aliases": ["Syntrix", "syntrix"],
      "extensions": [".sx"],
      "configuration": "./language-configuration.json",
      "icon": {
          "light": "./icon.svg",
          "dark": "./icon.svg"
      }
    }],
    "grammars": [{
      "language": "syntrix",
      "scopeName": "source.syntrix",
      "path": "./syntaxes/syntrix.tmLanguage.json"
    }]
  }
}
